# MongoDB connection string. Example for local MongoDB. Change as needed for your setup.
MONGO_URI=mongodb://localhost:27017/db

# Secret key for JWT authentication. Set this to your own secure value!
JWT_SECRET=your_jwt_secret_here

# Port for the backend server
PORT=5000

# Node environment
NODE_ENV=development

# URL where the frontend client runs
CLIENT_URL=http://localhost:3000
